<!doctype html>
<html lang="en" data-framework="javascript">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Gorethink â€¢ OMG</title>
        <link rel="stylesheet" href="/css/main.css">
    </head>
    <body>
        <section id="omgapp">
            <header id="header">
                <h1>OMGo.edu</h1>
            </header>
            <section id="main">
                <ul id="prog-list">
                    {{range .Name}}
                    <li data-id="{{.Id}}" class="{{.Status}}">
                        <div class="view">
                            <a href="/toggle/{{.Id}}" class="button toggle"></a>
                            <span>{{.Text}}</span>
                            <a href="/delete/{{.Id}}" class="button destroy"></a>
                        </div>
                    </li>
                    {{end}}
                </ul>
            </section>
            <footer id="footer">
                <span id="prog-count"></span>
                <ul id="filters">
                    <li>
                        <a href="/all">All</a>
                    </li>
                    <li>
                        <a href="/active">Active</a>
                    </li>
                    <li>
                        <a href="/completed">Completed</a>
                    </li>
                </ul>
                <a href="/clear" class="button" id="clear-completed">Clear completed</a>
            </footer>
        </section>
        <footer id="info">
            <p>More programs</p>
        </footer>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
      <!--   <script type="text/javascript">
            connect("{{.Route}}");
        </script> -->
    </body>
</html>
